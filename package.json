{
  "name": "gotoshi",
  "version": "1.0.0",
  "engines": {
    "node": "6.2.1"
  },
  "description": "A Bitcoin GO game",
  "scripts": {
    "build": "rimraf dist && webpack --bail --progress --profile",
    "server": "webpack-dev-server --history-api-fallback --inline --progress",
    "test": "karma start",
    "test-watch": "karma start --auto-watch --no-single-run",
    "develop": "npm run server",
    "start": "webcoin-bridge --network webcoin-bitcoin-testnet --port ${PORT:=8193}"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/Head/gotoshi.git"
  },
  "author": "Felix Schuchmann",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/Head/gotoshi/issues"
  },
  "homepage": "https://github.com/Head/gotoshi",
  "dependencies": {
    "angular": "^1.5.5",
    "angular-ui-router": "0.3.1",
    "angular-ui-bootstrap": "latest",
    "angular-bootstrap-show-errors": "latest",
    "bootstrap": "^3.3.5",
    "tenuki": "latest",
    "bitcoin-net": "latest",
    "bitcoin-filter": "latest",
    "bitcoin-inventory": "latest",
    "blockchain-spv": "latest",
    "blockchain-download": "latest",
    "webcoin-bitcoin": "latest",
    "webcoin-bitcoin-testnet": "latest",
    "webcoin-bridge": "latest",
    "bitcoinjs-lib": "https://github.com/bitcoinjs/bitcoinjs-lib/tarball/master",
    "levelup": "latest",
    "memdown": "latest",
    "level-js": "latest"
  },
  "devDependencies": {
    "angular-mocks": "^1.5.2",
    "autoprefixer": "^6.0.3",
    "babel-core": "^6.7.2",
    "babel-loader": "^6.2.4",
    "babel-preset-es2015": "^6.13.2",
    "babel-preset-stage-1": "^6.13.0",
    "copy-webpack-plugin": "^3.0.1",
    "css-loader": "^0.24.0",
    "extract-text-webpack-plugin": "^1.0.1",
    "file-loader": "^0.9.0",
    "html-webpack-plugin": "^2.7.1",
    "isparta-loader": "^2.0.0",
    "jasmine-core": "latest",
    "json-loader": "^0.5.4",
    "karma": "latest",
    "karma-chrome-launcher": "latest",
    "karma-coverage": "latest",
    "karma-jasmine": "latest",
    "karma-phantomjs-launcher": "latest",
    "karma-phantomjs-shim": "latest",
    "karma-sourcemap-loader": "latest",
    "karma-spec-reporter": "latest",
    "karma-webpack": "^1.7.0",
    "node-libs-browser": "latest",
    "null-loader": "^0.1.1",
    "phantomjs-prebuilt": "latest",
    "postcss-loader": "^0.11.0",
    "raw-loader": "^0.5.1",
    "rimraf": "^2.5.1",
    "style-loader": "^0.13.1",
    "webpack": "^1.13.2",
    "webpack-dev-server": "^1.14.1"
  }
}
